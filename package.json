{
  "name": "writejs",
  "version": "0.0.1",
  "description": "A WYSIWYG library that does not suck",
  "main": "dist/index.js",
  "author": "Lukas Bombach",
  "license": "MIT",
  "scripts": {
    "start": "webpack --progress --colors --watch --env=development",
    "lint": "eslint src *.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "webpack": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "no-underscore-dangle": "off",
      "max-len": "off"
    },
    "plugins": [
      "class-property"
    ],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "babel-eslint": "^7.2.3"
  }
}
